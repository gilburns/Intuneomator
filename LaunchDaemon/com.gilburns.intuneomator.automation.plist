<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>AssociatedBundleIdentifiers</key>
	<string>com.gilburns.Intuneomator</string>
	<key>Label</key>
	<string>com.gilburns.intuneomator.automation</string>
	<key>ProgramArguments</key>
	<array>
		<string>/Library/Application Support/Intuneomator/IntuneomatorService</string>
		<string>intune-automation</string>
	</array>
	<key>RunAtLoad</key>
	<false/>
	<key>StandardErrorPath</key>
	<string>/var/log/com.gilburns.intuneomator.automation.err.log</string>
	<key>StandardOutPath</key>
	<string>/var/log/com.gilburns.intuneomator.automation.out.log</string>
	<key>StartCalendarInterval</key>
	<array>
		<dict>
			<key>Hour</key>
			<integer>8</integer>
			<key>Minute</key>
			<integer>30</integer>
		</dict>
		<dict>
			<key>Hour</key>
			<integer>20</integer>
			<key>Minute</key>
			<integer>30</integer>
		</dict>
	</array>
	<key>WatchPaths</key>
	<array>
		<string>/Library/Application Support/Intuneomator/.automation.trigger</string>
	</array>
</dict>
</plist>
